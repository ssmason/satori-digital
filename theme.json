{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"customTemplates": [],
	"patterns": [],
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"palette": [
				{
					"color": "#b4a43b",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "azure",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#e8e2b6",
					"name": "Extra 1",
					"slug": "extra1"
				},
				{
					"color": "#fcf9e4",
					"name": "Extra2",
					"slug": "extra2"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "1100px",
			"wideSize": "1100px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "1",
					"size": "10px",
					"slug": "1"
				},
				{
					"name": "2",
					"size": "clamp(0.75rem, 0.5rem + 0.625vw, 1rem)",
					"slug": "2"
				},
				{
					"name": "3",
					"size": "clamp(1.25rem, 1rem + 0.625vw, 1.5rem)",
					"slug": "3"
				},
				{
					"name": "4",
					"size": "clamp(1.75rem, 1.5rem + 0.625vw, 2rem)",
					"slug": "4"
				},
				{
					"name": "5",
					"size": "clamp(2.25rem, 2rem + 0.625vw, 2.5rem)",
					"slug": "5"
				},
				{
					"name": "6",
					"size": "clamp(3rem, 2.5rem + 1.25vw, 3.5rem)",
					"slug": "6"
				},
				{
					"name": "7",
					"size": "clamp(5rem, 4rem + 2vw, 5.5em)",
					"slug": "7"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Arial, Helvetica,  sans-serif",
					"name": "Arial, Helvetica, Arial",
					"slug": "arial-helvetica-sansarif"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
					"name": "System fonts",
					"slug": "system-fonts"
				},
				{
					"fontFamily": "Georgia, serif",
					"name": "Georgia, serif",
					"slug": "georgia-serif"
				},
				{
					"fontFamily": "Roboto, serif",
					"name": "roboto, serif",
					"slug": "roboto-serif"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.25rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1rem"
					},
					"name": "Large",
					"size": "1rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.5rem"
					},
					"name": "XL",
					"size": "3rem",
					"slug": "xl-large"
				},
				{
					"fluid": {
						"max": "3.75rem",
						"min": "3rem"
					},
					"name": "XXL",
					"size": "3.75rem",
					"slug": "xxl-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/buttons": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--arial-helvetica-sansarif)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/column": {
				"spacing": {
					"padding": {
						"bottom": "0rem",
						"left": "0.5rem",
						"right": "0.5rem",
						"top": "0rem"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0rem",
						"left": "0",
						"right": "0",
						"top": "0rem"
					}
				}
			},
			"core/group": {
				"spacing": {
					"margin": {
						"bottom": "40px",
						"left": "0rem",
						"right": "0rem",
						"top": "40px"
					},
					"padding": {
						"bottom": "0",
						"left": "0rem",
						"right": "0rem",
						"top": "0"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--base)"
		},
		"elements": {
			"h2": {
				"typography": {
					"fontWeight": "300"
				}
			},
			"heading": {
				"color": {
					"text": "#6e8c46"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--20)",
			"margin": {
				"bottom": "0rem",
				"left": "0",
				"right": "0",
				"top": "0rem"
			},
			"padding": {
				"bottom": "0px",
				"left": "0",
				"right": "0",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "Roboto, serif",
			"lineHeight": "1.8"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "main-header",
			"title": "General Header"
		},
		{
			"area": "footer",
			"name": "general-footer",
			"title": "General Footer"
		},
		{
			"area": "general",
			"name": "query-loop",
			"title": "Query Loop"
		}
	],
	"version": 3
}